## Copy this file to .env and fill in real values (do NOT commit secrets)
# Server port
PORT=15500

# Admin credentials (required)
ADMIN_USER=admin
ADMIN_PASS=change_me_here

# Optional landing passcode used by the server-side gate
PASSCODE=replace_with_passcode

# SMTP (booking emails)
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_SECURE=false
SMTP_USER=you@example.com
SMTP_PASS=your_smtp_password
SMTP_FROM=no-reply@example.com
BOOKING_TO=you@example.com

# Optional: Redis URL for session persistence
# Example: redis://localhost:6379
REDIS_URL=

# Session TTL (ms)
#SESSION_TTL_MS=86400000

# Storage backend: local | s3
STORAGE_BACKEND=local

# S3 configuration (if using STORAGE_BACKEND=s3)
# Example: S3_BUCKET=my-bucket
S3_BUCKET=
S3_REGION=us-east-1
# If you prefer to serve objects via a custom domain/CDN, set S3_PUBLIC_URL
# e.g. https://cdn.example.com or leave blank to use default S3 URL
S3_PUBLIC_URL=
